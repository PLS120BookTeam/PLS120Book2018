set.seed(400)
x <- 0:10
Y <- x +rnorm(10)
plot(x, Y,  pch = 20, xaxt = "n", yaxt = "n", xlab = "X", ylab = "Y", xlim = c(0,10), ylim = c(0,10), bty = "l")
lines(0:10, 0:10, lwd = 2)
slopes <- seq(0, 1, .20)
b0 <- sort(seq(0,5, 1), decreasing = T)
for(i in 1:6){
y <-  b0[i] + x*slopes[i]
lines(0:10, y, lty = 3, lwd = 1.5)
}
