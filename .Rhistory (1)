zTable <- matrix(NA, nrow = 33, ncol = 10)
for (j in 0:9) {
zTable[i, j] <- 1 - pnorm(q = 1/10 + j/100)
}
zTable[i, j+1] <- 1 - pnorm(q = 1/10 + j/100)
zTable[i, j] <- 1 - pnorm(q = 1/10 + (j - 1)/100)
str(zTable)
for (i in 1:33) {
for (j in 1:10) {
print(zTable[i, j])
}
}
for (i in 1:33) {
for (j in 1:10) {
zTable[i, j]  <- 1 - pnorm(q = i/10 + (j - 1)/100)
}
}
zTable
